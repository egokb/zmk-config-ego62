#include <dt-bindings/zmk/matrix_transform.h>

/ {
    chosen {
        zmk,physical-layout = &physical_layout0;
    };

    key_physical_attrs: key_physical_attrs {
        compatible = "zmk,key-physical-attrs";
        #key-cells = <7>;
    };

    physical_layout0: physical_layout_0 {
        compatible = "zmk,physical-layout";
        display-name = "Ego62";
        keys
            = <&key_physical_attrs (19.05 * 0) (19.05 * 0) 0 0 0 0 0>    /* = (EQL) */
            , <&key_physical_attrs (19.05 * 1) (19.05 * 0) 0 0 0 0 0>    /* 1 */
            , <&key_physical_attrs (19.05 * 2) (19.05 * 0) 0 0 0 0 0>    /* 2 */
            , <&key_physical_attrs (19.05 * 3) (19.05 * 0) 0 0 0 0 0>    /* 3 */
            , <&key_physical_attrs (19.05 * 4) (19.05 * 0) 0 0 0 0 0>    /* 4 */
            , <&key_physical_attrs (19.05 * 5) (19.05 * 0) 0 0 0 0 0>    /* 5 */
            , <&key_physical_attrs (19.05 * 13) (19.05 * 0) 0 0 0 0 0>   /* 6 */
            , <&key_physical_attrs (19.05 * 14) (19.05 * 0) 0 0 0 0 0>   /* 7 */
            , <&key_physical_attrs (19.05 * 15) (19.05 * 0) 0 0 0 0 0>   /* 8 */
            , <&key_physical_attrs (19.05 * 16) (19.05 * 0) 0 0 0 0 0>   /* 9 */
            , <&key_physical_attrs (19.05 * 17) (19.05 * 0) 0 0 0 0 0>   /* 0 */
            , <&key_physical_attrs (19.05 * 18) (19.05 * 0) 0 0 0 0 0>   /* - (MINUS) */
            , <&key_physical_attrs (19.05 * 0) (19.05 * 1) 0 0 0 0 0>    /* TAB */
            , <&key_physical_attrs (19.05 * 1) (19.05 * 1) 0 0 0 0 0>    /* Q */
            , <&key_physical_attrs (19.05 * 2) (19.05 * 1) 0 0 0 0 0>    /* W */
            , <&key_physical_attrs (19.05 * 3) (19.05 * 1) 0 0 0 0 0>    /* E */
            , <&key_physical_attrs (19.05 * 4) (19.05 * 1) 0 0 0 0 0>    /* R */
            , <&key_physical_attrs (19.05 * 5) (19.05 * 1) 0 0 0 0 0>    /* T */
            , <&key_physical_attrs (19.05 * 13) (19.05 * 1) 0 0 0 0 0>   /* Y */
            , <&key_physical_attrs (19.05 * 14) (19.05 * 1) 0 0 0 0 0>   /* U */
            , <&key_physical_attrs (19.05 * 15) (19.05 * 1) 0 0 0 0 0>   /* I */
            , <&key_physical_attrs (19.05 * 16) (19.05 * 1) 0 0 0 0 0>   /* O */
            , <&key_physical_attrs (19.05 * 17) (19.05 * 1) 0 0 0 0 0>   /* P */
            , <&key_physical_attrs (19.05 * 18) (19.05 * 1) 0 0 0 0 0>   /* [ (LBKT) */
            , <&key_physical_attrs (19.05 * 0) (19.05 * 2) 0 0 0 0 0>    /* CAPS (CLCK) */
            , <&key_physical_attrs (19.05 * 1) (19.05 * 2) 0 0 0 0 0>    /* A */
            , <&key_physical_attrs (19.05 * 2) (19.05 * 2) 0 0 0 0 0>    /* S */
            , <&key_physical_attrs (19.05 * 3) (19.05 * 2) 0 0 0 0 0>    /* D */
            , <&key_physical_attrs (19.05 * 4) (19.05 * 2) 0 0 0 0 0>    /* F */
            , <&key_physical_attrs (19.05 * 5) (19.05 * 2) 0 0 0 0 0>    /* G */
            , <&key_physical_attrs (19.05 * 13) (19.05 * 2) 0 0 0 0 0>   /* H */
            , <&key_physical_attrs (19.05 * 14) (19.05 * 2) 0 0 0 0 0>   /* J */
            , <&key_physical_attrs (19.05 * 15) (19.05 * 2) 0 0 0 0 0>   /* K */
            , <&key_physical_attrs (19.05 * 16) (19.05 * 2) 0 0 0 0 0>   /* L */
            , <&key_physical_attrs (19.05 * 17) (19.05 * 2) 0 0 0 0 0>   /* ; (SEMI) */
            , <&key_physical_attrs (19.05 * 18) (19.05 * 2) 0 0 0 0 0>   /* ' (SQT) */
            , <&key_physical_attrs (19.05 * 0) (19.05 * 3) 0 0 0 0 0>    /* LSHIFT (LSFT) */
            , <&key_physical_attrs (19.05 * 1) (19.05 * 3) 0 0 0 0 0>    /* \ (NUBS) */
            , <&key_physical_attrs (19.05 * 2) (19.05 * 3) 0 0 0 0 0>    /* Z */
            , <&key_physical_attrs (19.05 * 3) (19.05 * 3) 0 0 0 0 0>    /* X */
            , <&key_physical_attrs (19.05 * 4) (19.05 * 3) 0 0 0 0 0>    /* C */
            , <&key_physical_attrs (19.05 * 5) (19.05 * 3) 0 0 0 0 0>    /* V */
            , <&key_physical_attrs (19.05 * 13) (19.05 * 3) 0 0 0 0 0>   /* B */
            , <&key_physical_attrs (19.05 * 14) (19.05 * 3) 0 0 0 0 0>   /* N */
            , <&key_physical_attrs (19.05 * 15) (19.05 * 3) 0 0 0 0 0>   /* M */
            , <&key_physical_attrs (19.05 * 16) (19.05 * 3) 0 0 0 0 0>   /* , (CMMA) */
            , <&key_physical_attrs (19.05 * 17) (19.05 * 3) 0 0 0 0 0>   /* . (DOT) */
            , <&key_physical_attrs (19.05 * 18) (19.05 * 3) 0 0 0 0 0>   /* / (FSLH) */
            , <&key_physical_attrs (19.05 * 3) (19.05 * 4) 0 0 0 0 0>    /* ~ */
            , <&key_physical_attrs (19.05 * 4) (19.05 * 4) 0 0 0 0 0>    /* / (FSLH) */
            , <&key_physical_attrs (19.05 * 5.5) (19.05 * 4) 0 0 0 0 0>  /* LALT */
            , <&key_physical_attrs (19.05 * 6.5) (19.05 * 4) 0 0 0 0 0>  /* SPACE (SPC) */
            , <&key_physical_attrs (19.05 * 7.5) (19.05 * 4) 0 0 0 0 0>  /* SPACE (SPC) */
            , <&key_physical_attrs (19.05 * 6.5) (19.05 * 5) 0 0 0 0 0>  /* SPACE (SPC) */
            , <&key_physical_attrs (19.05 * 7.5) (19.05 * 5) 0 0 0 0 0>  /* RALT */
            , <&key_physical_attrs (19.05 * 10.5) (19.05 * 5) 0 0 0 0 0> /* RCTL */
            , <&key_physical_attrs (19.05 * 11.5) (19.05 * 5) 0 0 0 0 0> /* RGUI */
            , <&key_physical_attrs (19.05 * 10.5) (19.05 * 4) 0 0 0 0 0> /* RCTL */
            , <&key_physical_attrs (19.05 * 11.5) (19.05 * 4) 0 0 0 0 0> /* RGUI */
            , <&key_physical_attrs (19.05 * 12.5) (19.05 * 4) 0 0 0 0 0> /* RCTL */
            , <&key_physical_attrs (19.05 * 14) (19.05 * 4) 0 0 0 0 0>   /* RGUI */
            , <&key_physical_attrs (19.05 * 15) (19.05 * 4) 0 0 0 0 0>   /* FN1 */
            ;
    };

    default_transform: matrix_transform {
        compatible = "zmk,matrix-transform";
        rows = <5>;
        columns = <14>;
        map = <
            RC(0,0) RC(0,1) RC(0,2) RC(0,3) RC(0,4) RC(0,5) RC(0,8) RC(0,9) RC(0,10) RC(0,11) RC(0,12) RC(0,13)
            RC(1,0) RC(1,1) RC(1,2) RC(1,3) RC(1,4) RC(1,5) RC(1,8) RC(1,9) RC(1,10) RC(1,11) RC(1,12) RC(1,13)
            RC(2,0) RC(2,1) RC(2,2) RC(2,3) RC(2,4) RC(2,5) RC(2,8) RC(2,9) RC(2,10) RC(2,11) RC(2,12) RC(2,13)
            RC(3,0) RC(3,1) RC(3,2) RC(3,3) RC(3,4) RC(3,5) RC(3,8) RC(3,9) RC(3,10) RC(3,11) RC(3,12) RC(3,13)
            RC(4,0) RC(4,1) RC(4,2) RC(4,3) RC(4,4) RC(4,5) RC(4,6) RC(4,7) RC(4,8) RC(4,9) RC(4,10) RC(4,11) RC(4,12) RC(4,13)
        >;
    };

    kscan0: kscan {
        compatible = "zmk,kscan-gpio-matrix";
        debounce-press-ms = <1>;
        debounce-release-ms = <1>;
        diode-direction = "col2row";
    };
};
