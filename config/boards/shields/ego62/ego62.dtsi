#include <dt-bindings/zmk/matrix_transform.h>
#include <physical_layouts.dtsi>

/ {
	chosen {
		zmk,physical-layout = <&physical_layout0>;
	};

	physical_layout0: physical_layout_0 {
        compatible = "zmk,physical-layout";
        display-name = "Ego62";
        keys
            /* Row 0 (top) */
            = <&key_physical_attrs 100 100    0    0 0 0 0>    /* = (EQL) */
            , <&key_physical_attrs 100 100  100    0 0 0 0>    /* 1 */
            , <&key_physical_attrs 100 100  200    0 0 0 0>    /* 2 */
            , <&key_physical_attrs 100 100  300    0 0 0 0>    /* 3 */
            , <&key_physical_attrs 100 100  400    0 0 0 0>    /* 4 */
            , <&key_physical_attrs 100 100  500    0 0 0 0>    /* 5 */
            , <&key_physical_attrs 100 100 1300    0 0 0 0>    /* 6 */
            , <&key_physical_attrs 100 100 1400    0 0 0 0>    /* 7 */
            , <&key_physical_attrs 100 100 1500    0 0 0 0>    /* 8 */
            , <&key_physical_attrs 100 100 1600    0 0 0 0>    /* 9 */
            , <&key_physical_attrs 100 100 1700    0 0 0 0>    /* 0 */
            , <&key_physical_attrs 100 100 1800    0 0 0 0>    /* - (MINUS) */
            /* Row 1 */
            , <&key_physical_attrs 100 100    0  100 0 0 0>    /* TAB */
            , <&key_physical_attrs 100 100  100  100 0 0 0>    /* Q */
            , <&key_physical_attrs 100 100  200  100 0 0 0>    /* W */
            , <&key_physical_attrs 100 100  300  100 0 0 0>    /* E */
            , <&key_physical_attrs 100 100  400  100 0 0 0>    /* R */
            , <&key_physical_attrs 100 100  500  100 0 0 0>    /* T */
            , <&key_physical_attrs 100 100 1300  100 0 0 0>    /* Y */
            , <&key_physical_attrs 100 100 1400  100 0 0 0>    /* U */
            , <&key_physical_attrs 100 100 1500  100 0 0 0>    /* I */
            , <&key_physical_attrs 100 100 1600  100 0 0 0>    /* O */
            , <&key_physical_attrs 100 100 1700  100 0 0 0>    /* P */
            , <&key_physical_attrs 100 100 1800  100 0 0 0>    /* [ (LBKT) */
            /* Row 2 */
            , <&key_physical_attrs 100 100    0  200 0 0 0>    /* CAPS (CLCK) */
            , <&key_physical_attrs 100 100  100  200 0 0 0>    /* A */
            , <&key_physical_attrs 100 100  200  200 0 0 0>    /* S */
            , <&key_physical_attrs 100 100  300  200 0 0 0>    /* D */
            , <&key_physical_attrs 100 100  400  200 0 0 0>    /* F */
            , <&key_physical_attrs 100 100  500  200 0 0 0>    /* G */
            , <&key_physical_attrs 100 100 1300  200 0 0 0>    /* H */
            , <&key_physical_attrs 100 100 1400  200 0 0 0>    /* J */
            , <&key_physical_attrs 100 100 1500  200 0 0 0>    /* K */
            , <&key_physical_attrs 100 100 1600  200 0 0 0>    /* L */
            , <&key_physical_attrs 100 100 1700  200 0 0 0>    /* ; (SEMI) */
            , <&key_physical_attrs 100 100 1800  200 0 0 0>    /* ' (SQT) */
            /* Row 3 */
            , <&key_physical_attrs 100 100    0  300 0 0 0>    /* LSHIFT (LSFT) */
            , <&key_physical_attrs 100 100  100  300 0 0 0>    /* \ (NUBS) */
            , <&key_physical_attrs 100 100  200  300 0 0 0>    /* Z */
            , <&key_physical_attrs 100 100  300  300 0 0 0>    /* X */
            , <&key_physical_attrs 100 100  400  300 0 0 0>    /* C */
            , <&key_physical_attrs 100 100  500  300 0 0 0>    /* V */
            , <&key_physical_attrs 100 100 1300  300 0 0 0>    /* B */
            , <&key_physical_attrs 100 100 1400  300 0 0 0>    /* N */
            , <&key_physical_attrs 100 100 1500  300 0 0 0>    /* M */
            , <&key_physical_attrs 100 100 1600  300 0 0 0>    /* , (CMMA) */
            , <&key_physical_attrs 100 100 1700  300 0 0 0>    /* . (DOT) */
            , <&key_physical_attrs 100 100 1800  300 0 0 0>    /* / (FSLH) */
            /* Row 4 (bottom) */
            , <&key_physical_attrs 100 100  300  400 0 0 0>    /* ~ */
            , <&key_physical_attrs 100 100  400  400 0 0 0>    /* / (FSLH) */
            , <&key_physical_attrs 100 100  550  400 0 0 0>    /* LALT */
            , <&key_physical_attrs 100 100  650  400 0 0 0>    /* SPACE (SPC) */
            , <&key_physical_attrs 100 100  750  400 0 0 0>    /* SPACE (SPC) */
            , <&key_physical_attrs 100 100  650  500 0 0 0>    /* SPACE (SPC) */
            , <&key_physical_attrs 100 100  750  500 0 0 0>    /* RALT */
            , <&key_physical_attrs 100 100 1050  500 0 0 0>    /* RCTL */
            , <&key_physical_attrs 100 100 1150  500 0 0 0>    /* RGUI */
            , <&key_physical_attrs 100 100 1050  400 0 0 0>    /* RCTL */
            , <&key_physical_attrs 100 100 1150  400 0 0 0>    /* RGUI */
            , <&key_physical_attrs 100 100 1250  400 0 0 0>    /* RCTL */
            , <&key_physical_attrs 100 100 1400  400 0 0 0>    /* RGUI */
            , <&key_physical_attrs 100 100 1500  400 0 0 0>    /* FN1 */
            ;
    };

    default_transform: matrix_transform {
        compatible = "zmk,matrix-transform";
        rows = <5>;
        columns = <14>;
        map = <
            RC(0,0) RC(0,1) RC(0,2) RC(0,3) RC(0,4) RC(0,5) RC(0,8) RC(0,9) RC(0,10) RC(0,11) RC(0,12) RC(0,13)
            RC(1,0) RC(1,1) RC(1,2) RC(1,3) RC(1,4) RC(1,5) RC(1,8) RC(1,9) RC(1,10) RC(1,11) RC(1,12) RC(1,13)
            RC(2,0) RC(2,1) RC(2,2) RC(2,3) RC(2,4) RC(2,5) RC(2,8) RC(2,9) RC(2,10) RC(2,11) RC(2,12) RC(2,13)
            RC(3,0) RC(3,1) RC(3,2) RC(3,3) RC(3,4) RC(3,5) RC(3,8) RC(3,9) RC(3,10) RC(3,11) RC(3,12) RC(3,13)
            RC(4,0) RC(4,1) RC(4,2) RC(4,3) RC(4,4) RC(4,5) RC(4,6) RC(4,7) RC(4,8) RC(4,9) RC(4,10) RC(4,11) RC(4,12) RC(4,13)
        >;
    };

    kscan0: kscan {
        compatible = "zmk,kscan-gpio-matrix";
        debounce-press-ms = <1>;
        debounce-release-ms = <1>;
        diode-direction = "col2row";
    };
};
